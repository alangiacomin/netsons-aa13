{
    "preset": "laravel",
    "rules": {
        "not_operator_with_successor_space": false,
        "no_extra_blank_lines": {
            "tokens": [
                "break",
                "continue",
                "curly_brace_block",
                "default",
                "extra",
                "parenthesis_brace_block",
                "return",
                "square_brace_block",
                "throw",
                "use",
                "use_trait"
            ]
        },
        "new_with_parentheses": {
            "anonymous_class": true,
            "named_class": true
        },
        "ordered_class_elements": {
            "order": [
                "use_trait",
                "case",
                "constant",
                "constant_public",
                "constant_protected",
                "constant_private",
                "property_public",
                "property_protected",
                "property_private",
                "construct",
                "destruct",
                "magic",
                "phpunit",
                "method_abstract",
                "method_public",
                "method_protected_static",
                "method_protected",
                "method_private_static",
                "method_private"
            ],
            "sort_algorithm": "none"
        }
    }
}
